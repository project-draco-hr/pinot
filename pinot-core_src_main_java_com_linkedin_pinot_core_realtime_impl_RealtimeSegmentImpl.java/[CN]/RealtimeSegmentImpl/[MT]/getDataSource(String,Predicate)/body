{
  DataSource ds=new RealtimeColumnDataSource(dataSchema.getFieldSpecFor(columnName),dictionaryMap.get(columnName),docIdMap,invertedIndexMap.get(columnName),columnName,docIdSearchableOffset,dataSchema,dimemsionTupleMap,metricsOffsetMap,maxNumberOfMultivaluesMap.get(columnName));
  ds.setPredicate(p);
  return ds;
}
