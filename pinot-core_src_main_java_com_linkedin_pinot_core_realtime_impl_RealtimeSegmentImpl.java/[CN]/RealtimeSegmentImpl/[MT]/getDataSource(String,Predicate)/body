{
  DataSource ds=new RealtimeColumnDataSource(dataSchema.getFieldSpecFor(columnName),dictionaryMap.get(columnName),docIdMap,invertedIndexMap.get(columnName),columnName,docIdSearchableOffset,dataSchema,dimemsionTupleMap,maxNumberOfMultivaluesMap.get(columnName),dimensionsSerde,metricsSerDe);
  ds.setPredicate(p);
  return ds;
}
