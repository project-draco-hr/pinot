{
  StarTreeRecord record=new StarTreeRecordImpl.Builder().setDimensionValue("A","A0").setDimensionValue("B","BX").setDimensionValue("C","CX").setTime(0L).setMetricValue("M",100L).build();
  recordStore.update(record);
  StarTreeQuery starTreeQuery=new StarTreeQueryImpl.Builder().setDimensionValue("A","?").setDimensionValue("B","?").setDimensionValue("C","?").setTimeBuckets(new HashSet<Long>(Arrays.asList(0L))).build();
  long[] metricSums=recordStore.getMetricSums(starTreeQuery);
  Assert.assertEquals(metricSums[0],100L);
}
