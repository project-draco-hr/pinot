{
  if (brokerRequest.isSetFilterQuery()) {
    return new RawFilterPlanNode(indexSegment,brokerRequest);
  }
  return new BaseFilterPlanNode(indexSegment,brokerRequest){
    @Override protected Operator constructPhysicalOperator(    FilterQueryTree filterQueryTree){
      return new StarTreeOperator(0,indexSegment.getTotalDocs() - indexSegment.getSegmentMetadata().getTotalAggregateDocs() - 1,null);
    }
  }
;
}
