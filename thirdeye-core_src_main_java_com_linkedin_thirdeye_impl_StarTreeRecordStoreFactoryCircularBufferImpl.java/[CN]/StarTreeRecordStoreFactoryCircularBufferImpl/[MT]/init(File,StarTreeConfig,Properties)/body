{
  this.dimensionSpecs=starTreeConfig.getDimensions();
  this.metricSpecs=starTreeConfig.getMetrics();
  this.rootDir=rootDir;
  this.numTimeBuckets=(int)starTreeConfig.getTime().getBucket().getUnit().convert(starTreeConfig.getTime().getRetention().getSize(),starTreeConfig.getTime().getRetention().getUnit());
}
