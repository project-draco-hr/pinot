{
  DataInputStream in=new DataInputStream(new ByteArrayInputStream(bytes));
  int length;
  byte[] b;
  length=in.readInt();
  b=new byte[length];
  in.readFully(b);
  UUID nodeId=UUID.fromString(new String(b));
  length=in.readInt();
  byte[] md5=new byte[length];
  in.readFully(md5);
  return new BootstrapPhaseMapOutputKey(nodeId,md5);
}
