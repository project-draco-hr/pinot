{
  if (query.getTimeRange() == null) {
    throw new IllegalArgumentException("Query must have time range");
  }
  StringBuilder sb=new StringBuilder();
  sb.append("/timeSeries").append("/").append(URLEncoder.encode(collection,"UTF-8")).append("/").append(URLEncoder.encode(metricName,"UTF-8")).append("/").append(query.getTimeRange().getStart()).append("/").append(query.getTimeRange().getEnd());
  return appendDimensions(sb,config,query).toString();
}
