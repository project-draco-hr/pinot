{
  for (  AggregationInfo aggregationInfo : brokerRequest.getAggregationsInfo()) {
    String aggregationType=aggregationInfo.getAggregationType();
    if (!aggregationType.equalsIgnoreCase("sum") && !aggregationType.equalsIgnoreCase("min") && !aggregationType.equalsIgnoreCase("max")&& !aggregationType.equalsIgnoreCase("avg")) {
      return false;
    }
  }
  return true;
}
