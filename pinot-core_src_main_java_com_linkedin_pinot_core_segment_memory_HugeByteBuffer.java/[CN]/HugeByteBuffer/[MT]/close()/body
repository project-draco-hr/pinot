{
  if (owner && buffers != null) {
    for (    ByteBuffer buffer : buffers) {
      MmapUtils.unloadByteBuffer(buffer);
    }
    if (raf != null) {
      try {
        raf.close();
      }
 catch (      IOException e) {
        LOGGER.error("Failed to close file: {}. Continuing with errors.",raf,e);
      }
    }
    buffers=null;
  }
}
