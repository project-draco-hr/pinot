{
  Double average=0.0;
  int numRows=0;
  for (  ResultTable.Row row : _rows) {
    average+=new Double(row.get(_column).toString());
    ++numRows;
  }
  average=average / numRows;
  ResultTable resultTable=new ResultTable(Collections.emptyList(),1);
  resultTable.add(0,average);
  return resultTable;
}
