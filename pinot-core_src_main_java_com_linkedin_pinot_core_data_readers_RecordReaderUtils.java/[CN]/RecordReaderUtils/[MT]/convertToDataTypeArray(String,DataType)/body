{
  Object[] value;
  if ((token == null) || (token.isEmpty())) {
    value=new Object[1];
    value[0]=getDefaultNullValue(dataType);
  }
 else {
    String[] tokens=token.split(",");
    value=new Object[tokens.length];
    for (int i=0; i < tokens.length; ++i) {
      value[i]=convertToDataType(tokens[i],dataType);
    }
  }
  return value;
}
