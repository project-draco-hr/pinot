{
switch (type) {
case SHORT:
    buffer.putShort(value.shortValue());
  break;
case INT:
buffer.putInt(value.intValue());
break;
case LONG:
buffer.putLong(value.longValue());
break;
case FLOAT:
buffer.putFloat(value.floatValue());
break;
case DOUBLE:
buffer.putDouble(value.doubleValue());
break;
}
}
