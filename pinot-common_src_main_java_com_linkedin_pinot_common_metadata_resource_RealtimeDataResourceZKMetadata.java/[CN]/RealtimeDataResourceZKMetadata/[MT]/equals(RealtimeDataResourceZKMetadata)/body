{
  if (!super.equals(anotherMetadata)) {
    return false;
  }
  if (anotherMetadata.getStreamType() != _streamType || !anotherMetadata.getDataSchema().toString().equals(_dataSchema.toString()) || !anotherMetadata.getStreamMetadata().toString().equals(_streamMetadata.toString())) {
    return false;
  }
  return true;
}
