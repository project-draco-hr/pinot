{
  if (field == null) {
    throw new IllegalArgumentException();
  }
switch (field) {
case SELECTION_COLUMNS:
    return isSetSelectionColumns();
case SELECTION_SORT_SEQUENCE:
  return isSetSelectionSortSequence();
case OFFSET:
return isSetOffset();
case SIZE:
return isSetSize();
}
throw new IllegalStateException();
}
