{
  if (_inputDataDir != null) {
    CreateSegmentCommand segmentCreator=new CreateSegmentCommand().setDataDir(_inputDataDir).setSchemaFile(_schemaFileName).setTableName(_tableName).setSegmentName(_segmentName).setOutputDir(_segmentDirName).setOverwrite(true).setEnableStarTreeIndex(_enableStarTreeIndex);
    segmentCreator.execute();
  }
}
