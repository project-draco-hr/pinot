{
  DateTime start=ISODateTimeFormat.dateTimeParser().parseDateTime("2016-01-01T00:00:00.000+00:00");
  DateTime end=ISODateTimeFormat.dateTimeParser().parseDateTime("2016-01-02T00:00:00.000+00:00");
  String betweenClause=PqlUtils.getBetweenClause(start,end,DEFAULT_TIME_SPEC);
  Assert.assertEquals(betweenClause,"timeColumn BETWEEN '403224' AND '403248'");
}
