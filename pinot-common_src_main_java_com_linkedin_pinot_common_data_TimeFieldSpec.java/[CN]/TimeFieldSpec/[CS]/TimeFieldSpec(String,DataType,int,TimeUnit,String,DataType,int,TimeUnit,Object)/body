{
  super(outgoingName,outgoingDataType,true,defaultNullValue);
  _incomingGranularitySpec=new TimeGranularitySpec(incomingDataType,incomingTimeUnitSize,incomingTimeUnit,incomingName);
  _outgoingGranularitySpec=new TimeGranularitySpec(outgoingDataType,outgoingTimeUnitSize,outgoingTimeUnit,outgoingName);
}
