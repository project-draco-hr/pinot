{
  final int rows=mmappedDataFile.getNumberOfRows();
  if (rows == 0) {
    return -1;
  }
  int low=0;
  int high=rows - 1;
  while (low <= high) {
    final int middle=(low + high) / 2;
    final String midValue=mmappedDataFile.getString(middle,col);
    if (midValue.compareTo(value) > 0) {
      high=middle - 1;
    }
 else     if (midValue.compareTo(value) < 0) {
      low=middle + 1;
    }
 else {
      return middle;
    }
  }
  return -(low + 1);
}
