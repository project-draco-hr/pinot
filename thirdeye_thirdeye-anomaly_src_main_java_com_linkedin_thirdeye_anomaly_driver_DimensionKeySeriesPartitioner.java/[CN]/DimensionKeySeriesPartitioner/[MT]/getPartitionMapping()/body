{
  int[] arr=new int[partitions];
  for (int i=0; i < arr.length; i++) {
    arr[i]=i;
  }
  if (seed != 0) {
    Random rand=new Random(seed);
    for (int i=0; i < arr.length; i++) {
      int j=rand.nextInt(arr.length);
      int tmp=arr[j];
      arr[j]=arr[i];
      arr[i]=tmp;
    }
  }
  return arr;
}
