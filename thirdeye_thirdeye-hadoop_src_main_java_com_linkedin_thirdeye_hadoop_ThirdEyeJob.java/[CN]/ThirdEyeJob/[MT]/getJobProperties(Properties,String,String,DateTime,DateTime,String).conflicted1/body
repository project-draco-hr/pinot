{
  Properties config=new Properties();
  config.setProperty(TopKPhaseConstants.TOPK_ROLLUP_PHASE_SCHEMA_PATH.toString(),getSchemaPath(root,collection));
  config.setProperty(TopKPhaseConstants.TOPK_ROLLUP_PHASE_CONFIG_PATH.toString(),getConfigPath(root,collection));
  config.setProperty(TopKPhaseConstants.TOPK_ROLLUP_PHASE_INPUT_PATH.toString(),inputPaths);
  config.setProperty(TopKPhaseConstants.TOPK_ROLLUP_PHASE_OUTPUT_PATH.toString(),getIndexDir(root,collection,minTime,maxTime) + File.separator + TOPK.getName());
  return config;
}
