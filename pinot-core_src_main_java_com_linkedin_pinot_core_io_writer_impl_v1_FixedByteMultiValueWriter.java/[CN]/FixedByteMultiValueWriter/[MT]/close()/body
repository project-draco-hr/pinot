{
  IOUtils.closeQuietly(raf);
  raf=null;
  MmapUtils.unloadByteBuffer(dataBuffer);
  dataBuffer=null;
  MmapUtils.unloadByteBuffer(headerBuffer);
  headerBuffer=null;
  dataWriter.close();
  dataWriter=null;
  headerWriter.close();
  headerWriter=null;
  headerReader.close();
  headerReader=null;
}
