{
  this.config=config;
  if (config != null) {
    for (    Map.Entry<Object,Object> entry : config.entrySet()) {
      String metricName=(String)entry.getKey();
      Integer thresholdValue=Integer.valueOf((String)entry.getValue());
      metricThresholdValues.put(metricName,thresholdValue);
    }
  }
}
