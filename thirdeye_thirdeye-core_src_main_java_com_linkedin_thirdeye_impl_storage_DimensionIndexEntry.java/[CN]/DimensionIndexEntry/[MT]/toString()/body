{
  StringBuilder sb=new StringBuilder();
  sb.append("nodeId=").append(nodeId).append("\t").append("fileId=").append(fileId).append("\t").append("dictionaryStartOffset=").append(dictionaryStartOffset).append("\t").append("dictionaryLength=").append(dictionaryLength).append("\t").append("bufferStartOffset=").append(bufferStartOffset).append("\t").append("bufferLength=").append(bufferLength).append("\t");
  return sb.toString();
}
