{
  LOGGER.info("active functions for rulebased");
  List<RuleBasedFunctionTableRow> rows;
  if (hideInactive) {
    rows=FunctionTable.selectActiveRows(config.getAnomalyDatabaseConfig(),RuleBasedFunctionTableRow.class,config.getCollectionName());
  }
 else {
    rows=FunctionTable.selectRows(config.getAnomalyDatabaseConfig(),RuleBasedFunctionTableRow.class,config.getCollectionName());
  }
  return new ActiveRuleBasedView(config.getAnomalyDatabaseConfig().getUrl(),config.getAnomalyDatabaseConfig().getFunctionTableName(),config.getCollectionName(),rows);
}
