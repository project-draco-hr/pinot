{
  LOGGER.info("active functions for generic");
  List<GenericFunctionTableRow> rows;
  if (hideInactive) {
    rows=FunctionTable.selectActiveRows(config.getAnomalyDatabaseConfig(),GenericFunctionTableRow.class,config.getCollectionName());
  }
 else {
    rows=FunctionTable.selectRows(config.getAnomalyDatabaseConfig(),GenericFunctionTableRow.class,config.getCollectionName());
  }
  return new ActiveGenericView(config.getAnomalyDatabaseConfig().getUrl(),config.getAnomalyDatabaseConfig().getFunctionTableName(),config.getCollectionName(),rows);
}
