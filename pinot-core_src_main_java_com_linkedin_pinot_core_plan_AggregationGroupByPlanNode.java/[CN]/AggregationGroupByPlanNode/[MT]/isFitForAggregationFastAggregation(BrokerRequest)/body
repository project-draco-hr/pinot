{
  for (  AggregationInfo aggregationInfo : brokerRequest.getAggregationsInfo()) {
    String aggregationType=aggregationInfo.getAggregationType();
    if (!aggregationType.equalsIgnoreCase("count") && !aggregationType.equalsIgnoreCase("sum") && !aggregationType.equalsIgnoreCase("min")&& !aggregationType.equalsIgnoreCase("max")&& !aggregationType.equalsIgnoreCase("avg")&& !aggregationType.equalsIgnoreCase("minmaxrange")) {
      return false;
    }
  }
  return true;
}
