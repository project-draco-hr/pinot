{
  StringBuilder b=new StringBuilder();
  for (  String key : _fieldMap.keySet()) {
    if (_fieldMap.get(key) instanceof Object[]) {
      b.append(key + " : " + Arrays.toString((Object[])_fieldMap.get(key))+ ", ");
    }
 else {
      b.append(key + " : " + _fieldMap.get(key)+ ", ");
    }
  }
  return b.toString();
}
