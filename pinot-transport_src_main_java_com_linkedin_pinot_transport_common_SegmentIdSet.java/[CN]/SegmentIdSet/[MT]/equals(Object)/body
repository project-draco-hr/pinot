{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  SegmentIdSet other=(SegmentIdSet)obj;
  if (_idSet == null) {
    if (other._idSet != null)     return false;
  }
 else   if (!_idSet.equals(other._idSet))   return false;
  return true;
}
