{
  final FileWriter fw=new FileWriter("/home/dpatel/experiments/data/lva1_queries_un_encoded");
  while ((line=br.readLine()) != null) {
    final String bql=(line.split("BQL:")[1]).split("Time")[0];
    fw.write(StringUtils.trim(bql));
    fw.write("\n");
  }
  br.close();
  fis.close();
  fw.close();
}
