{
  String retString=("AddTenant -controllerHost " + _controllerHost + " -controllerPort "+ _controllerPort+ " -name "+ _name+ " -role "+ _role+ " -instanceCount "+ _instanceCount+ " -offlineInstanceCount "+ _offlineInstanceCount+ " -realTimeInstanceCount "+ _realtimeInstanceCount);
  return ((_exec) ? (retString + " -exec") : retString);
}
