{
  sum_log_likelihood_=0;
  for (int ii=0; ii < number_of_observations_; ii++) {
    if (observations_ref_[ii] == null) {
      continue;
    }
    MultivariateNormalDistribution dist=new MultivariateNormalDistribution(predicted_observation_means_[ii].getColumn(0).toArray(),predicted_observation_covariances_[ii].toArray2());
    sum_log_likelihood_+=Math.log(dist.density(observations_ref_[ii].getColumn(0).toArray()));
  }
}
