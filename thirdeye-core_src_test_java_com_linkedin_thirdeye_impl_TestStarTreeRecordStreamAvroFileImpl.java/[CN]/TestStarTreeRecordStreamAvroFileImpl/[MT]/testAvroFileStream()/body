{
  Number metricSum=0;
  for (  StarTreeRecord record : new StarTreeRecordStreamAvroFileImpl(avroFile,Arrays.asList("A","B","C"),Arrays.asList("M"),Arrays.asList("INT"),"hoursSinceEpoch")) {
    metricSum=NumberUtils.sum(metricSum,record.getMetricValues().get("M"),"INT");
  }
  Assert.assertEquals(metricSum,100);
}
