{
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  DataOutputStream dos=new DataOutputStream(baos);
  byte[] bytes;
  bytes=dimensionValue.getBytes();
  dos.writeInt(bytes.length);
  dos.write(bytes);
  bytes=series.toBytes();
  dos.writeInt(bytes.length);
  dos.write(bytes);
  baos.close();
  dos.close();
  return baos.toByteArray();
}
