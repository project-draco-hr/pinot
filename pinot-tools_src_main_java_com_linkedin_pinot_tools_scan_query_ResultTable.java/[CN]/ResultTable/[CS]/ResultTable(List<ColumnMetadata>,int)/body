{
  _columnMetadatas=columns;
  _rows=new ArrayList<>(numRows);
  for (int i=0; i < numRows; ++i) {
    _rows.add(i,new Row());
  }
  int index=0;
  _columnMap=new HashMap<>();
  for (  ColumnMetadata metadata : columns) {
    if (metadata != null) {
      _columnMap.put(metadata.getColumnName(),index);
    }
    ++index;
  }
}
