{
  if (idsToOr.size() == 0) {
    return new MutableRoaringBitmap();
  }
  if (idsToOr.size() == 1) {
    return invertedIndex.getImmutable(idsToOr.get(0));
  }
  MutableRoaringBitmap b=new MutableRoaringBitmap();
  for (int i=0; i < idsToOr.size(); i++) {
    b.or(invertedIndex.getImmutable(idsToOr.get(i)));
  }
  return b;
}
