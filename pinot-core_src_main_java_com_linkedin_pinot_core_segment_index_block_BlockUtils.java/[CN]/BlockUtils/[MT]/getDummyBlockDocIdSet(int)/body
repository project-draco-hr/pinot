{
  return new BlockDocIdSet(){
    @Override public BlockDocIdIterator iterator(){
      return new BlockDocIdIterator(){
        int counter=0;
        @Override public int skipTo(        int targetDocId){
          if (targetDocId < maxDocId) {
            counter=targetDocId;
            return counter;
          }
 else {
            return Constants.EOF;
          }
        }
        @Override public int next(){
          if (counter >= maxDocId) {
            return Constants.EOF;
          }
          return counter++;
        }
        @Override public int currentDocId(){
          return counter;
        }
      }
;
    }
    @Override public Object getRaw(){
      throw new UnsupportedOperationException("cannot access raw in blockDocIds that are not backed by bitmaps");
    }
  }
;
}
