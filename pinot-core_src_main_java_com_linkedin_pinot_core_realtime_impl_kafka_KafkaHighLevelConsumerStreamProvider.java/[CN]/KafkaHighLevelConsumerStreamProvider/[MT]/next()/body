{
  if (kafkaIterator.hasNext()) {
    try {
      GenericRow row=decoder.decode(kafkaIterator.next().message());
      kafkaEventsConsumedCount.inc();
      return row;
    }
 catch (    Exception e) {
      LOGGER.warn("Caught exception while consuming events",e);
      kafkaEventsFailedCount.inc();
    }
  }
  return null;
}
