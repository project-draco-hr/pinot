{
  if (!(o instanceof ContextualEvent)) {
    return false;
  }
  ContextualEvent e=(ContextualEvent)o;
  return Objects.equals(getId(),e.getId()) && Objects.equals(timeUtc,e.getTimeUtc()) && Objects.equals(title,e.getTitle())&& Objects.equals(description,e.getDescription())&& Objects.equals(reference,e.getReference());
}
