{
  List<StarTreeRecord> records=new ArrayList<StarTreeRecord>();
  for (int i=1; i <= 10; i++) {
    StarTreeRecord record=new StarTreeRecordImpl.Builder().setDimensionValue("A","A1").setDimensionValue("B","B1").setDimensionValue("C","C1").setMetricValue("M",100L * i).build();
    records.add(record);
  }
  Properties props=new Properties();
  props.put("M","100");
  thresholdFunction.init(props);
  Assert.assertTrue(thresholdFunction.passesThreshold(records));
}
