{
  long numTimeBuckets=ThirdEyeClientUtils.getTimeBucketCount(request,dataTimeSpec.getBucket());
  return getPql(request.getCollection(),request.getRawMetricNames(),request.getStartTimeInclusive(),request.getEndTimeExclusive(),request.getDimensionValues(),request.getGroupBy(),dataTimeSpec,request.shouldGroupByTime(),request.getTopCount(),numTimeBuckets);
}
