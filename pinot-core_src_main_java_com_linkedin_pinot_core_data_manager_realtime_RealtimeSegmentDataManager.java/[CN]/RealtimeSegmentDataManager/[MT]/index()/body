{
  if (counter % 100000 == 0) {
    logger.info("indexed : " + counter);
  }
  if (keepIndexing) {
    counter++;
    keepIndexing=((RealtimeSegmentImpl)realtimeSegment).index(kafkaStreamProvider.next());
    return keepIndexing;
  }
  logger.info("keepIndexing = false, stop indexing!");
  return false;
}
