{
  if (!(o instanceof SmtpConfiguration)) {
    return false;
  }
  SmtpConfiguration at=(SmtpConfiguration)o;
  return Objects.equals(smtpHost,at.getSmtpHost()) && Objects.equals(smtpPort,at.getSmtpPort()) && Objects.equals(smtpUser,at.getSmtpUser())&& Objects.equals(smtpPassword,at.getSmtpPassword());
}
