{
  Matcher matcher=RATIO_REGEX.matcher(expr);
  if (matcher.matches()) {
    String first=matcher.group(1);
    String second=matcher.group(2);
    this.singularValue=RATIO;
    this.arguments=trimQuotes(Arrays.asList(first,second));
  }
 else {
    this.singularValue=expr;
    this.arguments=null;
  }
}
