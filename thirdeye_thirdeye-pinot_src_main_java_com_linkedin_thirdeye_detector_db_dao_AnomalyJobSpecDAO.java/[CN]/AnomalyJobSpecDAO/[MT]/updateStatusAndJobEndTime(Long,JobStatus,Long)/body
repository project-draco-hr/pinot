{
  try {
    int numRowsUpdated=namedQuery("com.linkedin.thirdeye.anomaly.AnomalyJobSpec#updateStatusAndJobEndTime").setParameter("id",id).setParameter("status",status).setParameter("jobEndTime",jobEndTime).executeUpdate();
    return numRowsUpdated == 1;
  }
 catch (  HibernateException e) {
    LOG.error("Exception in updateStatusAndJobEndTime",e);
    return false;
  }
}
