{
  Map<String,Collection<String>> dimensions;
  HttpGet httpGet=new HttpGet(URI.create(uri));
  HttpResponse response=httpClient.execute(httpGet);
  try {
    dimensions=objectMapper.readValue(response.getEntity().getContent(),new TypeReference<Map<String,Collection<String>>>(){
    }
);
  }
  finally {
    EntityUtils.consume(response.getEntity());
  }
  LOGGER.info("Cached dimension values for {}: {}",uri,dimensions);
  return dimensions;
}
