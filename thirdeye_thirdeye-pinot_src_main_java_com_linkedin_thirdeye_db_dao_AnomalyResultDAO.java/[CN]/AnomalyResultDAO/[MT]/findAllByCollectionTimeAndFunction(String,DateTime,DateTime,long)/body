{
  return getEntityManager().createQuery(FIND_BY_COLLECTION_TIME_AND_FUNCTION_ID,entityClass).setParameter("collection",collection).setParameter("startTimeUtc",startTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("endTimeUtc",endTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("functionId",functionId).getResultList();
}
