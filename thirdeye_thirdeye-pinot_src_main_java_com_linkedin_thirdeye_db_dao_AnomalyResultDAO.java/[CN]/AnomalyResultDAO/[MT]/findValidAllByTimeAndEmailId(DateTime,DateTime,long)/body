{
  return getEntityManager().createQuery(FIND_VALID_BY_TIME_EMAIL_ID,entityClass).setParameter("startTimeUtc",startTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("endTimeUtc",endTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("emailId",emailId).setParameter("dataMissing",false).getResultList();
}
