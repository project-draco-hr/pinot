{
  if (_controllerHost == null) {
    _controllerHost=NetUtil.getHostAddress();
  }
  if (!_exec) {
    LOGGER.warn("Dry Running Command: " + toString());
    LOGGER.warn("Use the -exec option to actually execute the command.");
    return true;
  }
  File schemaFile=new File(_schemaFile);
  LOGGER.info("Executing command: " + toString());
  if (!schemaFile.exists()) {
    throw new FileNotFoundException("file does not exist, + " + _schemaFile);
  }
  Schema s=Schema.fromFile(schemaFile);
  FileUploadUtils.sendFile(_controllerHost,_controllerPort,"schemas",s.getSchemaName(),new FileInputStream(schemaFile),schemaFile.length());
  return true;
}
