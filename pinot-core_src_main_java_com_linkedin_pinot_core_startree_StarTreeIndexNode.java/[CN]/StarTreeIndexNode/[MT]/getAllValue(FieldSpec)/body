{
  Object allValue;
switch (spec.getDataType()) {
case INT:
    allValue=spec.getDefaultNullValue();
  break;
case LONG:
allValue=spec.getDefaultNullValue();
break;
case FLOAT:
allValue=spec.getDefaultNullValue();
break;
case DOUBLE:
allValue=spec.getDefaultNullValue();
;
break;
case STRING:
case BOOLEAN:
allValue=spec.getDefaultNullValue();
;
break;
default :
throw new UnsupportedOperationException("unsupported data type : " + spec.getDataType() + " : "+ " for column : "+ spec.getName());
}
return allValue;
}
