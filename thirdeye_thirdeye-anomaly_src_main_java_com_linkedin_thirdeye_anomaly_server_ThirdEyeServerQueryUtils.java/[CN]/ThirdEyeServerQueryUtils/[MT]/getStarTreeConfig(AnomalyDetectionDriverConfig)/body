{
  String urlString="http://" + driverConfig.getThirdEyeServerHost() + ":"+ driverConfig.getThirdEyeServerPort()+ "/collections/"+ driverConfig.getCollectionName();
  LOGGER.info("getting star-tree : {}",urlString);
  if (enableCache) {
    StarTreeConfig cached=starTreeCache.get(urlString);
    if (cached != null) {
      LOGGER.info("cache hit for {}",urlString);
      return cached;
    }
  }
  URL url=new URL(urlString);
  StarTreeConfig result=OBJECT_MAPPER.readValue(new InputStreamReader(url.openStream(),"UTF-8"),StarTreeConfig.class);
  if (enableCache) {
    starTreeCache.put(urlString,result);
  }
  return result;
}
