{
  StringBuilder sb=new StringBuilder();
  sb.append("[");
  String delim="";
  for (  DimensionSpec spec : dimensionSpecs) {
    sb.append(delim).append(spec.getName()).append(":").append(record.getDimensionValues().get(spec.getName()));
    delim=",";
  }
  sb.append("]");
  return sb.toString();
}
