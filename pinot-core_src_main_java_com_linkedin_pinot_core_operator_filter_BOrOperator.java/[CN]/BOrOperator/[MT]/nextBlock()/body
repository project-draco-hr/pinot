{
  final Block[] blocks=new Block[operators.length];
  int i=0;
  boolean isAnyBlockEmpty=false;
  for (  final Operator operator : operators) {
    final Block nextBlock=operator.nextBlock();
    if (nextBlock == null) {
      isAnyBlockEmpty=true;
    }
    blocks[i++]=nextBlock;
  }
  if (isAnyBlockEmpty) {
    return null;
  }
  return new OrBlock(blocks);
}
