{
  Schema schema=Schema.fromFile(schemaFile);
  String brokerTenant="hybridBroker";
  String realtimeTenant="hybridServer";
  createBrokerTenant(brokerTenant);
  createServerTenant(realtimeTenant,1,1);
  addSchema(schemaFile,schema.getSchemaName());
  addHybridTable(resourceName,timeColumnName,timeColumnType,kafkaZkUrl,kafkaTopic,schema.getSchemaName(),realtimeTenant,brokerTenant,avroFile);
}
