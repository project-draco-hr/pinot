{
  for (int i=0; i < numIter; i++) {
    PinotDataBuffer buf=null;
    try {
      buf=ColumnIndexDirectoryTestHelper.newIndexBuffer(columnDirectory,column,size,i);
      int numValues=size / 4;
      for (int j=0; j < numValues; j++) {
        buf.putInt(j * 4,j);
      }
    }
  finally {
      if (buf != null) {
        buf.close();
      }
    }
  }
}
