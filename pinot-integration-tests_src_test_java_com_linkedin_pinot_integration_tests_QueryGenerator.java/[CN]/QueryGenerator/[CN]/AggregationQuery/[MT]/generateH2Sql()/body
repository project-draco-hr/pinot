{
  List<String> queries=new ArrayList<String>();
  if (_groupColumns.isEmpty()) {
    for (    String aggregateColumnAndFunction : _aggregateColumnsAndFunctions) {
      queries.add(joinWithSpaces("SELECT",aggregateColumnAndFunction,"FROM",_h2TableName,_predicate.generatePql(),_limit.generatePql()));
    }
  }
 else {
    for (    String aggregateColumnAndFunction : _aggregateColumnsAndFunctions) {
      queries.add(joinWithSpaces("SELECT",joinWithCommas(_groupColumns) + ",",aggregateColumnAndFunction,"FROM",_h2TableName,_predicate.generatePql(),"GROUP BY",joinWithCommas(_groupColumns),_limit.generatePql()));
    }
  }
  return queries;
}
