{
  if (SegmentVersion.v1.name().equals(segmentVersion)) {
    multiValuedReadBenchMarkV1(fwdIndexFile,totalDocs,totalNumberOfEntries,maxNumberOfMultiValues,bitsPerElement);
  }
 else   if (SegmentVersion.v2.name().equals(segmentVersion)) {
    multiValuedReadBenchMarkV2(fwdIndexFile,totalDocs,totalNumberOfEntries,maxNumberOfMultiValues,bitsPerElement);
  }
}
