{
  if (timeRange != null) {
    Set<Long> times=new HashSet<Long>();
    for (long i=timeRange.getStart(); i <= timeRange.getEnd(); i++) {
      times.add(i);
    }
    return times;
  }
 else {
    Set<Long> times=new HashSet<Long>();
    for (long i=getMinTime(); i <= getMaxTime(); i++) {
      times.add(i);
    }
    return times;
  }
}
