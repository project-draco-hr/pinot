{
  int middle=-1;
  if (minTime >= timeRangeStartValue) {
    middle=0;
  }
 else {
    int low=0;
    int high=length - 1;
    boolean found=false;
    while (low <= high) {
      middle=(low + high) / 2;
      buffer.position(startOffset + (middle * sizePerEntry));
      final long midValue=buffer.getLong();
      if (timeRangeStartValue > midValue) {
        low=middle + 1;
      }
 else       if (timeRangeStartValue < midValue) {
        high=middle - 1;
      }
 else {
        found=true;
        break;
      }
    }
  }
  return middle;
}
