{
  CreateSegmentCommand segmentBuilder=new CreateSegmentCommand();
  segmentBuilder.setDataDir(_dataDir.getAbsolutePath()).setFormat(FileFormat.CSV).setSchemaFile(_schemaFile.getAbsolutePath()).setTableName(_tableName).setSegmentName(_segmentName).setOutputDir(new File(_tempDir,"segments").getAbsolutePath()).setOverwrite(true);
  segmentBuilder.execute();
}
