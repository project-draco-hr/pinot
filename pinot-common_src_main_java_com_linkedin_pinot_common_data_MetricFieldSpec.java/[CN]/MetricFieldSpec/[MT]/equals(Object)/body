{
  if (this == object) {
    return true;
  }
  if (object instanceof MetricFieldSpec) {
    MetricFieldSpec that=(MetricFieldSpec)object;
    return getName().equals(that.getName()) && getDataType() == that.getDataType() && getDefaultNullValue().equals(that.getDefaultNullValue()) && getFieldSize() == that.getFieldSize() && derivedMetricType == that.derivedMetricType;
  }
  return false;
}
