{
  if (this == o) {
    return true;
  }
  if (!(o instanceof SegmentSizeInfo)) {
    return false;
  }
  SegmentSizeInfo that=(SegmentSizeInfo)o;
  if (diskSizeInBytes != that.diskSizeInBytes) {
    return false;
  }
  return segmentName != null ? segmentName.equals(that.segmentName) : that.segmentName == null;
}
