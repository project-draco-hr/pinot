{
  String encodedCollection=URLEncoder.encode(collection,ENCODING);
  return URI_JOINER.join(WEB_HDFS_PREFIX + hdfsUri.getPath(),encodedCollection,"data_" + timeRange.getStart() + "-"+ timeRange.getEnd(),"startree_generation","star-tree-" + encodedCollection,encodedCollection + "-tree.bin?op=OPEN");
}
