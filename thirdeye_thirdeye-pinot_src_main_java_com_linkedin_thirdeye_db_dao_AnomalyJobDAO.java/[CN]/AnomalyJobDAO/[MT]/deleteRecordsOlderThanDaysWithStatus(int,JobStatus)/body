{
  DateTime expireDate=new DateTime().minusDays(days);
  Timestamp expireTimestamp=new Timestamp(expireDate.getMillis());
  List<AnomalyJobSpec> anomalyJobSpecs=getEntityManager().createQuery(FIND_BY_STATUS_AND_LAST_MODIFIED_TIME_LT_EXPIRE,entityClass).setParameter("expireTimestamp",expireTimestamp).setParameter("status",status).getResultList();
  for (  AnomalyJobSpec anomalyJobSpec : anomalyJobSpecs) {
    delete(anomalyJobSpec);
  }
  return anomalyJobSpecs.size();
}
