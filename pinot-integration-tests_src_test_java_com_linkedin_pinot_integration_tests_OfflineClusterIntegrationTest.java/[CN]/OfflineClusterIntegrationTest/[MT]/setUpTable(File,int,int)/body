{
  String brokerTenant="offlineBroker";
  String offlineTenant="offlineServer";
  createBrokerTenant(brokerTenant,numBroker);
  createServerTenant(offlineTenant,numOffline,0);
  addSchema(schemaFile,"schemaFile");
