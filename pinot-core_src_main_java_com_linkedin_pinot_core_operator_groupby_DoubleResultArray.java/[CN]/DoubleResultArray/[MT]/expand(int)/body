{
  Preconditions.checkArgument(newSize > _doubles.length);
  Preconditions.checkState(newSize <= ResultHolderFactory.MAX_NUM_GROUP_KEYS_FOR_ARRAY_BASED,"Illegal request to expand DoubleArray beyond maximum limit (1M): " + newSize);
  double[] tmp=_doubles;
  _doubles=new double[newSize];
  System.arraycopy(tmp,0,_doubles,0,tmp.length);
  Arrays.fill(_doubles,tmp.length,newSize,_defaultValue);
}
