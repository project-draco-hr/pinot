{
  int nodeId=0;
  Queue<StarTreeIndexNode> queue=new LinkedList<StarTreeIndexNode>();
  queue.add(starTree);
  while (!queue.isEmpty()) {
    StarTreeIndexNode current=queue.remove();
    current.setNodeId(nodeId++);
    if (!current.isLeaf()) {
      for (      StarTreeIndexNode child : current.getChildren().values()) {
        queue.add(child);
      }
    }
  }
}
