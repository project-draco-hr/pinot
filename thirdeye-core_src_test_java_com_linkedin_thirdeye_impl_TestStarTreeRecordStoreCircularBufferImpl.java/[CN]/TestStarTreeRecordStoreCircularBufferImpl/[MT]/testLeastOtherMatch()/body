{
  StarTreeRecord record=new StarTreeRecordImpl.Builder().setDimensionValue("A","AX").setDimensionValue("B","B0").setDimensionValue("C","C0").setMetricValue("M",1).setMetricType("M",MetricType.INT).setTime(0L).build();
  recordStore.update(record);
  StarTreeQuery query=new StarTreeQueryImpl.Builder().setDimensionValue("A",StarTreeConstants.OTHER).setDimensionValue("B",StarTreeConstants.OTHER).setDimensionValue("C",StarTreeConstants.OTHER).build();
  Number[] sums=recordStore.getMetricSums(query);
  Assert.assertEquals(sums[0],1);
}
