{
  if (inString == null) {
    return null;
  }
switch (type) {
case INT:
    return new Integer(Integer.parseInt(inString));
case LONG:
  return new Long(Long.parseLong(inString));
case FLOAT:
return new Float(Float.parseFloat(inString));
case DOUBLE:
return new Double(Double.parseDouble(inString));
case BOOLEAN:
case STRING:
return inString.toString();
default :
return null;
}
}
