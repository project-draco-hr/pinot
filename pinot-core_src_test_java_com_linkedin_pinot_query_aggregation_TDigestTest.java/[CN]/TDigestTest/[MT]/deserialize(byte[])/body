{
  ByteArrayInputStream bais=new ByteArrayInputStream(bytes);
  ObjectInputStream ois=null;
  try {
    ois=new ObjectInputStream(bais);
    TDigest digest=(TDigest)ois.readObject();
    ois.close();
    return digest;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ClassNotFoundException e) {
    e.printStackTrace();
  }
  return null;
}
