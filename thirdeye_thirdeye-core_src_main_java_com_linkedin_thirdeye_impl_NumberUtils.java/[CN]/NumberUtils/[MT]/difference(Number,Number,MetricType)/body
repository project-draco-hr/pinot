{
switch (type) {
case SHORT:
    return a.shortValue() - b.shortValue();
case INT:
  return a.intValue() - b.intValue();
case LONG:
return a.longValue() - b.longValue();
case FLOAT:
return a.floatValue() - b.floatValue();
case DOUBLE:
return a.doubleValue() - b.doubleValue();
default :
return null;
}
}
