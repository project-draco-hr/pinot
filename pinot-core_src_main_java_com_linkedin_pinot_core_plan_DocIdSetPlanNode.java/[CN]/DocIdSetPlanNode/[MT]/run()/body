{
  if (_projectOp == null) {
    if (_filterNode != null) {
      _projectOp=new BDocIdSetOperator(_filterNode.run(),_indexSegment,_maxDocPerAggregation);
    }
 else {
      _projectOp=new BDocIdSetOperator(null,_indexSegment,_maxDocPerAggregation);
    }
    return _projectOp;
  }
 else {
    return _projectOp;
  }
}
