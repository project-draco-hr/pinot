{
  if (topKDimensionValues.getTopKDimensions().size() > 0) {
    FSDataOutputStream topKDimensionValuesOutputStream=fileSystem.create(new Path(configuration.get(TOPK_ROLLUP_PHASE2_OUTPUT_PATH.toString()) + "/" + context.getTaskAttemptID()+ ".stat"));
    OBJECT_MAPPER.writeValue(topKDimensionValuesOutputStream,topKDimensionValues);
    topKDimensionValuesOutputStream.close();
  }
}
