{
  List<String> metricNames=new ArrayList<>(metrics.size());
  for (  MetricSpec metric : metrics) {
    metricNames.add(metric.getName());
  }
  return String.format("AGGREGATE_%d_%s(%s)",aggregationGranularity.getSize(),aggregationGranularity.getUnit().toString().toUpperCase(),COMMA.join(metricNames));
}
