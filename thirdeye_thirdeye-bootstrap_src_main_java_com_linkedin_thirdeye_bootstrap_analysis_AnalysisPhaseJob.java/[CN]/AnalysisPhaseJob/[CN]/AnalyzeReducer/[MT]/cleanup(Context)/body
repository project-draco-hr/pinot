{
  FileSystem fileSystem=FileSystem.get(context.getConfiguration());
  Path outputPath=new Path(context.getConfiguration().get(AnalysisJobConstants.ANALYSIS_OUTPUT_PATH.toString()),AnalysisJobConstants.ANALYSIS_FILE_NAME.toString());
  OutputStream os=fileSystem.create(outputPath,true);
  os.write(aggregatedStats.toBytes());
  os.flush();
  os.close();
}
