{
  final Map<String,String> props=new HashMap<String,String>();
  props.put(CONFIG_PREFIX_OF_BROKER_TAG + tag + "."+ CommonConstants.Broker.TagResource.NUM_BROKER_INSTANCES,numBrokerInstances + "");
