{
  if (a == null) {
    return b;
  }
 else   if (b == null) {
    return a;
  }
  Number[] result=new Number[a.length];
  for (int i=0; i < a.length; i++) {
    double aVal=(a[i] == null ? 0.0 : a[i].doubleValue());
    double bVal=(b[i] == null ? 0.0 : b[i].doubleValue());
    result[i]=aVal + bVal;
  }
  return result;
}
