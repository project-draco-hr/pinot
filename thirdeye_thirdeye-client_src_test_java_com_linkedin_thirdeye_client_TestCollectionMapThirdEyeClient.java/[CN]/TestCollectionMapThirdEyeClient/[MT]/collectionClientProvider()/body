{
  initMap();
  ArrayList<Object[]> entries=new ArrayList<Object[]>();
  for (  ThirdEyeClient client : clients) {
    for (    String collection : client.getCollections()) {
      String collectionKey=prefixMap.get(client) + CollectionMapThirdEyeClient.CollectionKey.SEPARATOR + collection;
      entries.add(new Object[]{collectionKey,client,collection});
    }
  }
  return entries.toArray(new Object[entries.size()][]);
}
