{
  for (int i=0; i < numRows; i++) {
    if (!Arrays.equals(expected[i],actual[i])) {
      System.err.println("MisMatch");
      System.err.println("\t\texpected:" + Arrays.toString(expected[i]));
      System.err.println("\t\tactual  :" + Arrays.toString(actual[i]));
      return false;
    }
  }
  return true;
}
