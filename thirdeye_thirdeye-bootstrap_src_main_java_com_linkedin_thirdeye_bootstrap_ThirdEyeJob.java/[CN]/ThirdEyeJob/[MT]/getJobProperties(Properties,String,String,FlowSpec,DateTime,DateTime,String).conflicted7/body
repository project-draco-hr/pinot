{
  Properties config=new Properties();
  config.setProperty(AnalysisJobConstants.ANALYSIS_INPUT_AVRO_SCHEMA.toString(),getSchemaPath(root,collection));
  config.setProperty(AnalysisJobConstants.ANALYSIS_CONFIG_PATH.toString(),getConfigPath(root,collection));
  config.setProperty(AnalysisJobConstants.ANALYSIS_INPUT_PATH.toString(),inputPaths);
  config.setProperty(AnalysisJobConstants.ANALYSIS_OUTPUT_PATH.toString(),getMetricIndexDir(root,collection,flowSpec,minTime,maxTime) + File.separator + ANALYSIS.getName());
  return config;
}
