{
  Properties config=new Properties();
  config.setProperty(StarTreeBootstrapPhaseOneConstants.STAR_TREE_BOOTSTRAP_CONFIG_PATH.toString(),getConfigPath(root,collection));
  config.setProperty(StarTreeBootstrapPhaseOneConstants.STAR_TREE_BOOTSTRAP_INPUT_AVRO_SCHEMA.toString(),getSchemaPath(root,collection));
  config.setProperty(StarTreeBootstrapPhaseOneConstants.STAR_TREE_GENERATION_OUTPUT_PATH.toString(),getLatestTreeDirPath(root,collection) + File.separator + STARTREE_GENERATION.getName());
  config.setProperty(StarTreeBootstrapPhaseOneConstants.STAR_TREE_BOOTSTRAP_INPUT_PATH.toString(),inputPaths);
  config.setProperty(StarTreeBootstrapPhaseOneConstants.STAR_TREE_BOOTSTRAP_OUTPUT_PATH.toString(),getTimeDir(root,collection,flowSpec,minTime,maxTime) + File.separator + STARTREE_BOOTSTRAP_PHASE1.getName());
  return config;
}
