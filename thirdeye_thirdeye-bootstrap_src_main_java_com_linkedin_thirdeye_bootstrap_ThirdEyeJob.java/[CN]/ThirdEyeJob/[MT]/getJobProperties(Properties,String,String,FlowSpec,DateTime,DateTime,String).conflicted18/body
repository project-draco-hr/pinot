{
  Properties config=new Properties();
  config.setProperty(TopKRollupPhaseThreeConstants.TOPK_ROLLUP_PHASE3_CONFIG_PATH.toString(),getConfigPath(root,collection));
  config.setProperty(TopKRollupPhaseThreeConstants.TOPK_ROLLUP_PHASE3_INPUT_PATH.toString(),getMetricIndexDir(root,collection,flowSpec,minTime,maxTime) + File.separator + AGGREGATION.getName());
  config.setProperty(TopKRollupPhaseThreeConstants.TOPK_ROLLUP_PHASE3_OUTPUT_PATH.toString(),getMetricIndexDir(root,collection,flowSpec,minTime,maxTime) + File.separator + TOPK_ROLLUP_PHASE3.getName());
  config.setProperty(TopKRollupPhaseThreeConstants.TOPK_DIMENSIONS_PATH.toString(),getMetricIndexDir(root,collection,flowSpec,minTime,maxTime) + File.separator + TOPK_ROLLUP_PHASE2.getName());
  return config;
}
