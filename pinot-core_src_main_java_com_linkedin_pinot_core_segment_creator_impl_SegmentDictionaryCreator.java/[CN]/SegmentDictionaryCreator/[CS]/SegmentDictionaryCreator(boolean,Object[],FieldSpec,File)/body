{
  LOGGER.info("Creating segment for column {}, hasNulls = {}, cardinality = {}, dataType = {}, single value field = {}",spec.getName(),hasNulls,sortedList.length,spec.getDataType(),spec.isSingleValueField());
  this.sortedList=sortedList;
  this.spec=spec;
  dictionaryFile=new File(indexDir,spec.getName() + ".dict");
  FileUtils.touch(dictionaryFile);
}
