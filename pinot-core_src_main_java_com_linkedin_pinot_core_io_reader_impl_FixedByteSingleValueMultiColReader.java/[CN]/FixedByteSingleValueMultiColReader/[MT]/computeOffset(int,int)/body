{
  if (row >= rows || col >= cols) {
    final String message=String.format("Input (%d,%d) is not with in expected range (%d,%d for column:%s)",row,col,rows,cols,dataFile.getAbsolutePath());
    throw new IndexOutOfBoundsException(message);
  }
  final int offset=row * rowSize + colOffSets[col];
  return offset;
}
