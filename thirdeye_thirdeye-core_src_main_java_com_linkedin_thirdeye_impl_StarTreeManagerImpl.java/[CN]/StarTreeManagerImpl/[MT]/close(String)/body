{
synchronized (trees) {
    Map<File,StarTree> starTrees=trees.remove(collection);
    if (starTrees != null) {
      for (      StarTree starTree : starTrees.values()) {
        starTree.close();
      }
      LOGGER.info("Closed trees for collection {}",collection);
    }
    openCollections.remove(collection);
  }
}
