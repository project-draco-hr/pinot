{
  double estimateMean=0;
  double count=0;
  for (int ii=0; ii < seasonal + 1; ii++) {
    if (processTrainingTimeSeries[ii] != null) {
      count=count + 1;
      estimateMean=estimateMean + processTrainingTimeSeries[ii].get(0,0);
    }
  }
  estimateMean=estimateMean / count;
  return estimateMean;
}
