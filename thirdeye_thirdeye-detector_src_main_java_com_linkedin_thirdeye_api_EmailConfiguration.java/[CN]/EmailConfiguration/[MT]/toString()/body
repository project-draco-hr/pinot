{
  return MoreObjects.toStringHelper(this).add("collection",collection).add("metric",metric).add("fromAddress",fromAddress).add("toAddresses",toAddresses).add("cron",cron).add("smtpHost",smtpHost).add("smtpPort",smtpPort).add("smtpUser",smtpUser).add("windowSize",windowSize).add("windowUnit",windowUnit).add("isActive",isActive).toString();
}
