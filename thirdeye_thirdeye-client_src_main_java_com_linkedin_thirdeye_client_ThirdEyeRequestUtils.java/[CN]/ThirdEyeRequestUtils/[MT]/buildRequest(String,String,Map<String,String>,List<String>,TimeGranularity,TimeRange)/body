{
  DateTime start=new DateTime(timeRange.getStart(),DateTimeZone.UTC);
  start=start.withMillisOfDay(0);
  DateTime end=new DateTime(timeRange.getEnd(),DateTimeZone.UTC);
  ThirdEyeMetricFunction metricFunction=new ThirdEyeMetricFunction(aggregationGranularity,metricNames);
  return new ThirdEyeRequestBuilder().setCollection(collection).setMetricFunction(metricFunction).setStartTime(start).setEndTime(end).setDimensionValues(fixedDimensionValues).setGroupBy(groupByDimension).build();
}
