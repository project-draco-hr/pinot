{
  float lowerInFloat=Float.parseFloat(lower);
  float upperInFloat=Float.parseFloat(upper);
  float valueToCompare=getFloat(indexOfValueToCompare);
  boolean ret=true;
  if (includeLower) {
    if (valueToCompare < lowerInFloat)     ret=false;
  }
 else {
    if (valueToCompare <= lowerInFloat)     ret=false;
  }
  if (includeUpper) {
    if (valueToCompare > upperInFloat)     ret=false;
  }
 else {
    if (valueToCompare >= upperInFloat)     ret=false;
  }
  return ret;
}
