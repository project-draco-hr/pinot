{
  for (int i=0; i < arr.length; i++) {
    if (Double.isNaN(arr[i])) {
      double sum=0.0;
      int count=0;
      if (i - 1 >= 0 && !Double.isNaN(arr[i - 1])) {
        sum+=arr[i - 1];
        count++;
      }
      if (i + 1 < arr.length && !Double.isNaN(arr[i + 1])) {
        sum+=arr[i + 1];
        count++;
      }
      arr[i]=sum / count;
    }
  }
}
