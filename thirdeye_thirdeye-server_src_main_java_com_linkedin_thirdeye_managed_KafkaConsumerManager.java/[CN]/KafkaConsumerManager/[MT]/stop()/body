{
  for (  String collection : consumers.keySet()) {
    try {
      stop(collection);
    }
 catch (    FileNotFoundException e) {
      LOG.warn("No {} for {}",StarTreeConstants.KAFKA_CONFIG_FILE_NAME,collection);
    }
catch (    Exception e) {
      LOG.error("Could not stop consumer for {}",collection,e);
    }
  }
}
