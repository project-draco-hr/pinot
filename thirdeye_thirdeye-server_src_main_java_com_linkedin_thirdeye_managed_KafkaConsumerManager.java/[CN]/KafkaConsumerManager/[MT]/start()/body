{
  for (  String collection : starTreeManager.getCollections()) {
    try {
      start(collection);
    }
 catch (    FileNotFoundException e) {
      LOG.warn("No {} for {}",StarTreeConstants.KAFKA_CONFIG_FILE_NAME,collection);
    }
catch (    Exception e) {
      LOG.error("Could not start consumer for {}",collection,e);
    }
  }
}
