{
  if (!(incoming instanceof DataResource)) {
    return false;
  }
  if (((DataResource)incoming).getResourceName().equals(resourceName) && (((DataResource)incoming).getNumberOfBrokerInstances() == numberOfBrokerInstances) && (((DataResource)incoming).getBrokerTagName().equals(brokerTagName))) {
    return true;
  }
  return false;
}
