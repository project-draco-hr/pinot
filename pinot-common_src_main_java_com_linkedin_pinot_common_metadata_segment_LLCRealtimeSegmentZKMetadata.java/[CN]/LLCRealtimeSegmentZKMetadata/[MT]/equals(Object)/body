{
  if (isSameReference(this,segmentMetadata)) {
    return true;
  }
  if (isNullOrNotSameClass(this,segmentMetadata)) {
    return false;
  }
  LLCRealtimeSegmentZKMetadata metadata=(LLCRealtimeSegmentZKMetadata)segmentMetadata;
  return super.equals(metadata) && isEqual(_startOffset,metadata._startOffset) && isEqual(_endOffset,metadata._endOffset)&& isEqual(_numReplicas,metadata._numReplicas);
}
