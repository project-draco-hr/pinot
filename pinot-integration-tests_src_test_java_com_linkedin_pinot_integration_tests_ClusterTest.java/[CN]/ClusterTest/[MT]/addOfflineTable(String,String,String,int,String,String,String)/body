{
  JSONObject request=ControllerRequestBuilder.buildCreateOfflineTableJSON(resourceName,serverTenant,brokerTenant,timeColumnName,"DAYS",retentionTimeUnit,String.valueOf(retentionTimeValue),3,"BalanceNumSegmentAssignmentStrategy");
