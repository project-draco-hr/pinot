{
  try {
    GenericData.Record avroRecord=_reader.read(null,_decoderFactory.createBinaryDecoder(payload,0,payload.length,null));
    return _rowGenerator.transform(avroRecord,_avroSchema);
  }
 catch (  Exception e) {
    LOGGER.error("Caught exception",e);
    throw new RuntimeException(e);
  }
}
