{
  this.yamlObjectMapper=new ObjectMapper(new YAMLFactory());
  this.customDashboardCache=CacheBuilder.newBuilder().expireAfterWrite(1,TimeUnit.MINUTES).build(new CustomDashboardSpecLoader());
  this.dimensionGroupCache=CacheBuilder.newBuilder().expireAfterWrite(1,TimeUnit.MINUTES).build(new DimensionGroupSpecLoader());
}
