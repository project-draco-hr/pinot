{
  Connection conn=null;
  Statement stmt=null;
  try {
    conn=getConnection();
    stmt=conn.createStatement();
    stmt.execute(sql);
  }
 catch (  SQLException e) {
    throw e;
  }
 finally {
    try {
      if (conn != null) {
        conn.close();
      }
      if (stmt != null) {
        stmt.close();
      }
    }
 catch (    SQLException e) {
      e.printStackTrace();
    }
  }
}
