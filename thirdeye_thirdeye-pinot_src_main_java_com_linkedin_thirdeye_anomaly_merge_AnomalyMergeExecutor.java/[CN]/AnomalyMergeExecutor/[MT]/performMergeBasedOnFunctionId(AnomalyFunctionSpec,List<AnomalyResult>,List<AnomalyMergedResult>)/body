{
  AnomalyMergedResult latestMergedResult=mergedResultDAO.findLatestByFunctionIdOnly(function.getId());
  List<AnomalyMergedResult> mergedResults=AnomalyTimeBasedSummarizer.mergeAnomalies(latestMergedResult,unmergedResults,mergeConfig.getMergeDuration(),mergeConfig.getSequentialAllowedGap());
  for (  AnomalyMergedResult mergedResult : mergedResults) {
    mergedResult.setFunction(function);
  }
  output.addAll(mergedResults);
}
