{
  if (val == null) {
switch (spec) {
case DOUBLE:
      return V1Constants.Numbers.NULL_DOUBLE;
case FLOAT:
    return V1Constants.Numbers.NULL_FLOAT;
case INT:
  return V1Constants.Numbers.NULL_INT;
case LONG:
return V1Constants.Numbers.NULL_LONG;
default :
return V1Constants.Str.NULL_STRING;
}
}
return val;
}
