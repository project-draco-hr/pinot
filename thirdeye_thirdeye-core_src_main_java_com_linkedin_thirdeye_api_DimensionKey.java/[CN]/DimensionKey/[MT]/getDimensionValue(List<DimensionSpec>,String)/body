{
  for (int i=0; i < dimensionSpecs.size(); i++) {
    if (dimensionSpecs.get(i).getName().equals(dimensionName)) {
      return dimensionValues[i];
    }
  }
  throw new WebApplicationException(Response.status(Response.Status.BAD_REQUEST).header("No dimension ",dimensionName).entity("No dimension : " + dimensionName).build());
}
