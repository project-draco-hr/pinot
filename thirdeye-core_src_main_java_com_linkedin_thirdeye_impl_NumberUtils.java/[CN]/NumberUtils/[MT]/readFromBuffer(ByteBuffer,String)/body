{
  if ("SHORT".equals(type)) {
    return buffer.getShort();
  }
  if ("INT".equals(type)) {
    return buffer.getInt();
  }
  if ("LONG".equals(type)) {
    return buffer.getLong();
  }
  if ("FLOAT".equals(type)) {
    return buffer.getFloat();
  }
  if ("DOUBLE".equals(type)) {
    return buffer.getDouble();
  }
  return null;
}
