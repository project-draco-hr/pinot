{
  ThirdEyeRequest request=requestGenerator.generateRequestsForAggregation(timeSeriesRequest);
  Map<ThirdEyeRequest,ThirdEyeResponse> queryResultMap=queryCache.getQueryResultsAsyncAndWait(Collections.singletonList(request),DEFAULT_QUERY_TIMEOUT);
  TimeSeriesRow row=responseParser.parseAggregationOnlyResponse(timeSeriesRequest,queryResultMap);
  return row;
}
