{
  Configuration resourceMetadata=new PropertiesConfiguration();
  String columnNames=null;
  for (int i=0; i < 10; ++i) {
    if (columnNames == null) {
      columnNames=("col" + i);
    }
 else {
      columnNames+=(", col" + i);
    }
    resourceMetadata.addProperty(PREFIX_OF_KEY_OF_LOADING_INVERTED_INDEX + "table" + i,columnNames);
  }
  return resourceMetadata;
}
