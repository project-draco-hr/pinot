{
  if (type == Type.INT) {
    return INT;
  }
  if (type == Type.LONG) {
    return LONG;
  }
  if (type == Type.STRING || type == Type.BOOLEAN) {
    return STRING;
  }
  if (type == Type.FLOAT) {
    return FLOAT;
  }
  if (type == Type.DOUBLE) {
    return DOUBLE;
  }
  throw new UnsupportedOperationException(type.toString());
}
