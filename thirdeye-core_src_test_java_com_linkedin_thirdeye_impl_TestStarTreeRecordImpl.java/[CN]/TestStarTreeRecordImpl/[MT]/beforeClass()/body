{
  config=StarTreeConfig.decode(ClassLoader.getSystemResourceAsStream("sample-config.yml"));
  MetricTimeSeries timeSeries=new MetricTimeSeries(MetricSchema.fromMetricSpecs(config.getMetrics()));
  timeSeries.set(100L,"M",1);
  record=new StarTreeRecordImpl.Builder().setDimensionKey(getDimensionKey("A0","B0","C0")).setMetricTimeSeries(timeSeries).build(config);
}
