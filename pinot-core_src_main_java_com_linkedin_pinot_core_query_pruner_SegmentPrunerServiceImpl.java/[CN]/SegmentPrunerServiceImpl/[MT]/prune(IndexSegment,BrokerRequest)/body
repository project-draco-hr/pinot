{
  if (_segmentPrunerSet == null || _segmentPrunerSet.size() == 0) {
    return false;
  }
  for (  SegmentPruner pruner : _segmentPrunerSet) {
    if (pruner.prune(segment,brokerRequest)) {
      LOGGER.info(pruner + " pruned segment: " + segment.getSegmentName());
      return true;
    }
  }
  return false;
}
