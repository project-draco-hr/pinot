{
  return list(namedQuery("com.linkedin.thirdeye.api.AnomalyResult#findAllByCollectionTimeMetricAndDimensions").setParameter("collection",collection).setParameter("startTimeUtc",startTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("endTimeUtc",endTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("metric",metric).setParameterList("dimensions",dimensions));
