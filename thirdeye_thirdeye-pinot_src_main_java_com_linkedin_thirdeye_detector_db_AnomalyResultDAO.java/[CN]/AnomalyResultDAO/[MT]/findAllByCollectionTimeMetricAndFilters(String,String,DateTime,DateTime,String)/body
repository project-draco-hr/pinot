  return list(namedQuery("com.linkedin.thirdeye.api.AnomalyResult#findAllByCollectionTimeMetricAndFilters").setParameter("collection",collection).setParameter("startTimeUtc",startTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("endTimeUtc",endTime.toDateTime(DateTimeZone.UTC).getMillis()).setParameter("metric",metric).setParameter("filters",filters));
