{
  final Set<String> tableNameSet=new HashSet<String>();
  tableNameSet.addAll(Arrays.asList(HelixHelper.getResourceConfigsFor(_helixClusterName,resourceName,_helixAdmin).get("tableName").trim().split(",")));
  LOGGER.info("Current holding tables for resource: " + resourceName + " is "+ Arrays.toString(tableNameSet.toArray(new String[0])));
  return tableNameSet;
}
