{
  boolean done=false;
  if ((null != response)) {
    LOG.debug("Error got from {} is : {}",name,response);
    _delayedResponse=response;
    _error=null;
    done=true;
  }
 else   if (null != error) {
    LOG.debug("Error got from {} is : {}",name,error);
    _error=error;
  }
  return done;
}
