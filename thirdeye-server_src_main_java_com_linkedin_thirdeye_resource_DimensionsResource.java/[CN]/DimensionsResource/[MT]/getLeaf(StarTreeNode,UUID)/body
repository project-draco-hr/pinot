{
  if (node.isLeaf()) {
    return id.equals(node.getId()) ? node : null;
  }
 else {
    for (    StarTreeNode child : node.getChildren()) {
      StarTreeNode leaf=getLeaf(child,id);
      if (leaf != null) {
        return leaf;
      }
    }
    StarTreeNode leaf=getLeaf(node.getOtherNode(),id);
    if (leaf != null) {
      return leaf;
    }
    leaf=getLeaf(node.getStarNode(),id);
    if (leaf != null) {
      return leaf;
    }
    return null;
  }
}
