{
synchronized (sync) {
    buffer.clear();
    while (buffer.position() < buffer.limit()) {
      for (      DimensionSpec dimensionSpec : dimensionSpecs) {
        buffer.getInt();
      }
      for (int i=0; i < numTimeBuckets; i++) {
        buffer.getLong();
        for (        MetricSpec spec : metricSpecs) {
          buffer.putInt(0);
        }
      }
    }
  }
}
