{
  Set<Long> timeBuckets;
  if (query.getTimeRange() != null) {
    timeBuckets=new HashSet<Long>();
    for (long time=query.getTimeRange().getStart(); time <= query.getTimeRange().getEnd(); time++) {
      timeBuckets.add(time);
    }
  }
 else {
    timeBuckets=null;
  }
  return timeBuckets;
}
